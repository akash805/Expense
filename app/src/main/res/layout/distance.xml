                    android:layout_height="1dp"
                    android:layout_marginTop="3dp"
                    android:layout_marginStart="9dp"
                    android:layout_marginEnd="9dp"
                    android:background="#E4DDDD"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="11.64dp"
                        android:layout_height="11.98dp"
                        android:layout_marginTop="0.2dp"
                        android:layout_marginStart="11dp"
                        android:src="@drawable/vector"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        android:text="Fare Rate 1"
                        android:textColor="#1242D2"

                        android:textSize="10sp" />


                    <ImageView
                        android:layout_width="11dp"
                        android:layout_height="13dp"
                        android:layout_marginTop="0.2dp"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/sheet"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3dp"
                        android:text="Upto Km"
                        android:textColor="#1242D2"

                        android:textSize="10sp" />

                    <ImageView
                        android:layout_width="11dp"
                        android:layout_height="13dp"
                        android:layout_marginTop="0.2dp"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/vector"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3dp"
                        android:text="Fare Rate 2"
                        android:textColor="#1242D2"

                        android:textSize="10sp" />

                    <ImageView
                        android:layout_width="11dp"
                        android:layout_height="13dp"
                        android:layout_marginTop="0.2dp"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/sheet"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3dp"
                        android:text="Upto Km"
                        android:textColor="#1242D2"
                        android:textSize="10sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="1dp"
                    android:layout_marginStart="27dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="11.5"
                        android:textColor="@color/black"
                        android:textSize="8sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="62dp"
                        android:text="10Km"
                        android:textColor="@color/black"
                        android:textSize="8sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="48dp"
                        android:text="11.5"
                        android:textColor="@color/black"
                        android:textSize="8sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="62dp"
                        android:text="10Km"
                        android:textColor="@color/black"
                        android:textSize="8sp" />


                </LinearLayout>


            </LinearLayout>




        </com.google.android.material.card.MaterialCardView>



</LinearLayout>